{
  "manifest_version": 2,
  "name": "BalabobaTwitter",
  "background": {
    "scripts": ["background.js"]
  },
  "version": "1.0.0",
  "permissions": [
	"storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
	  "css" : [
			"warning.css"
		],
      "js": [
			"main.js"
      ],
      "run_at": "document_end"
    }
  ],
  	"browser_action": {
		"default_popup": "index.html"
	},
	
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
	
	
}
